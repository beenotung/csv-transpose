  "files": [
    "bundle.js"
  ],
  "devDependencies": {
    "@beenotung/tslib": "^18.0.0",
    "@types/node": "^17.0.21",
    "esbuild": "^0.14.23",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beenotung/csv-transpose.git"
  },
  "bugs": {
    "url": "https://github.com/beenotung/csv-transpose/issues"
  },
  "homepage": "https://github.com/beenotung/csv-transpose#readme"
